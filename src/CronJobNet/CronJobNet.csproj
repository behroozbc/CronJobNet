<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;net5.0;net6.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<Product>$(AssemblyName) ($(TargetFramework))</Product>
		<AssemblyVersion>1.0.0.0</AssemblyVersion>
		<AssemblyFileVersion>1.0.0.0</AssemblyFileVersion>
		<RepositoryUrl>https://github.com/behroozbc/CronJobNet</RepositoryUrl>
		<Authors>Behrooz Bozorg Chami</Authors>
		<PackageId>CronJobNet</PackageId>
		<PackOnBuild>true</PackOnBuild>
		<Owners>Behrooz Bozorg chami</Owners>
		<Summary>Help to have CronJob in dotNet</Summary>
		<Description>Help to have CronJob in dotNet</Description>
		<PackageTags>Cron,CronJobs</PackageTags>
		<Title>CronJobNet</Title>
	</PropertyGroup>

</Project>
